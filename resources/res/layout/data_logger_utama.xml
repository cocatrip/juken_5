<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent" android:keepScreenOn="true">
    <ScrollView android:id="@+id/scroll1" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_above="@+id/scrollText">
        <RelativeLayout android:id="@+id/parentPlot" android:background="#000000" android:layout_width="match_parent" android:layout_height="match_parent">
            <com.androidplot.xy.XYPlot android:id="@+id/tpsPlot" android:layout_width="match_parent" android:layout_height="250dp" app:lineLabels="9" app:title="" app:backgroundColor="#000000" app:domainTitle="" app:rangeTitle="TPS" app:lineLabelRotationBottom="0" app:lineLabelTextSizeLeft="13dp" app:graphBackgroundColor="#000000" style="@style/APDefacto"/>
            <com.androidplot.xy.XYPlot android:id="@+id/rpmPlot" android:layout_width="match_parent" android:layout_height="250dp" android:layout_below="@+id/tpsPlot" app:lineLabels="9" app:title="" app:backgroundColor="#000000" app:domainTitle="" app:rangeTitle="RPM" app:lineLabelRotationBottom="0" app:lineLabelTextSizeLeft="13dp" app:graphBackgroundColor="#000000" style="@style/APDefacto"/>
            <com.androidplot.xy.XYPlot android:id="@+id/afrPlot" android:layout_width="match_parent" android:layout_height="250dp" android:layout_below="@+id/rpmPlot" app:lineLabels="9" app:title="" app:backgroundColor="#000000" app:domainTitle="" app:rangeTitle="RPM" app:lineLabelRotationBottom="0" app:lineLabelTextSizeLeft="13dp" app:graphBackgroundColor="#000000" style="@style/APDefacto"/>
            <com.androidplot.xy.XYPlot android:id="@+id/mapPlot" android:layout_width="match_parent" android:layout_height="250dp" android:layout_below="@+id/afrPlot" app:lineLabels="9" app:title="" app:backgroundColor="#000000" app:domainTitle="" app:rangeTitle="RPM" app:lineLabelRotationBottom="0" app:lineLabelTextSizeLeft="13dp" app:graphBackgroundColor="#000000" style="@style/APDefacto"/>
            <com.androidplot.xy.XYPlot android:id="@+id/fuelPlot" android:layout_width="match_parent" android:layout_height="250dp" android:layout_below="@+id/mapPlot" app:lineLabels="9" app:title="" app:backgroundColor="#000000" app:domainTitle="" app:rangeTitle="RPM" app:lineLabelRotationBottom="0" app:lineLabelTextSizeLeft="13dp" app:graphBackgroundColor="#000000" style="@style/APDefacto"/>
            <com.androidplot.xy.XYPlot android:id="@+id/bmPlot" android:layout_width="match_parent" android:layout_height="250dp" android:layout_below="@+id/fuelPlot" app:lineLabels="9" app:title="" app:backgroundColor="#000000" app:domainTitle="" app:rangeTitle="RPM" app:lineLabelRotationBottom="0" app:lineLabelTextSizeLeft="13dp" app:graphBackgroundColor="#000000" style="@style/APDefacto"/>
            <com.androidplot.xy.XYPlot android:id="@+id/itPlot" android:layout_width="match_parent" android:layout_height="250dp" android:layout_below="@+id/bmPlot" app:lineLabels="9" app:title="" app:backgroundColor="#000000" app:domainTitle="" app:rangeTitle="RPM" app:lineLabelRotationBottom="0" app:lineLabelTextSizeLeft="13dp" app:graphBackgroundColor="#000000" style="@style/APDefacto"/>
            <com.androidplot.xy.XYPlot android:id="@+id/igPlot" android:layout_width="match_parent" android:layout_height="250dp" android:layout_below="@+id/itPlot" app:lineLabels="9" app:title="" app:backgroundColor="#000000" app:domainTitle="" app:rangeTitle="RPM" app:lineLabelRotationBottom="0" app:lineLabelTextSizeLeft="13dp" app:graphBackgroundColor="#000000" style="@style/APDefacto"/>
            <com.androidplot.xy.XYPlot android:id="@+id/eotPlot" android:layout_width="match_parent" android:layout_height="250dp" android:layout_below="@+id/igPlot" app:lineLabels="9" app:title="" app:backgroundColor="#000000" app:domainTitle="" app:rangeTitle="RPM" app:lineLabelRotationBottom="0" app:lineLabelTextSizeLeft="13dp" app:graphBackgroundColor="#000000" style="@style/APDefacto"/>
            <com.androidplot.xy.XYPlot android:id="@+id/batPlot" android:layout_width="match_parent" android:layout_height="250dp" android:layout_below="@+id/eotPlot" app:lineLabels="9" app:title="" app:backgroundColor="#000000" app:domainTitle="" app:rangeTitle="RPM" app:lineLabelRotationBottom="0" app:lineLabelTextSizeLeft="13dp" app:graphBackgroundColor="#000000" style="@style/APDefacto"/>
        </RelativeLayout>
    </ScrollView>
    <HorizontalScrollView android:id="@+id/scrollText" android:background="#ffffff" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="3dp" android:layout_alignParentBottom="true">
        <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content">
            <Button android:textSize="13dp" android:id="@+id/ButtonClick" android:background="#bcbbbb" android:layout_width="60dp" android:layout_height="30dp" android:layout_marginLeft="3dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="Click" android:layout_alignParentLeft="true"/>
            <Button android:textSize="13dp" android:id="@+id/ButtonVDyno" android:background="#bcbbbb" android:layout_width="60dp" android:layout_height="30dp" android:layout_marginLeft="3dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="V-Dyno" android:layout_toRightOf="@+id/ButtonClick"/>
            <TextView android:textSize="13dp" android:textColor="#000000" android:id="@+id/textRPM" android:layout_width="85dp" android:layout_height="25dp" android:layout_marginLeft="5dp" android:text="RPM : 16000" android:layout_toRightOf="@+id/ButtonVDyno" android:layout_alignParentTop="true"/>
            <TextView android:textSize="13dp" android:textColor="#000000" android:id="@+id/textTPS" android:layout_width="80dp" android:layout_height="25dp" android:text="TPS : 100" android:layout_toRightOf="@+id/textRPM" android:layout_alignBaseline="@+id/textRPM"/>
            <TextView android:textSize="13dp" android:textColor="#000000" android:id="@+id/textF" android:layout_width="70dp" android:layout_height="25dp" android:text="F : -100" android:layout_toRightOf="@+id/textTPS" android:layout_alignBaseline="@+id/textRPM"/>
            <TextView android:textSize="13dp" android:textColor="#000000" android:id="@+id/textBM" android:layout_width="80dp" android:layout_height="25dp" android:text="BM : 20.00" android:layout_toRightOf="@+id/textF" android:layout_alignBaseline="@+id/textRPM"/>
            <TextView android:textSize="13dp" android:textColor="#000000" android:id="@+id/textIG" android:layout_width="80dp" android:layout_height="25dp" android:text="IG : 90.0" android:layout_toRightOf="@+id/textBM" android:layout_alignBaseline="@+id/textRPM"/>
            <TextView android:textSize="13dp" android:textColor="#000000" android:id="@+id/textAFR" android:layout_width="85dp" android:layout_height="25dp" android:text="AFR : 14.0" android:layout_below="@+id/textRPM" android:layout_alignLeft="@+id/textRPM"/>
            <TextView android:textSize="13dp" android:textColor="#000000" android:id="@+id/textMAP" android:layout_width="80dp" android:layout_height="25dp" android:text="MAP : 100" android:layout_toRightOf="@+id/textAFR" android:layout_alignBaseline="@+id/textAFR"/>
            <TextView android:textSize="13dp" android:textColor="#000000" android:id="@+id/textIT" android:layout_width="70dp" android:layout_height="25dp" android:text="IT : 720" android:layout_toRightOf="@+id/textMAP" android:layout_alignBaseline="@+id/textAFR"/>
            <TextView android:textSize="13dp" android:textColor="#000000" android:id="@+id/textEOT" android:layout_width="80dp" android:layout_height="25dp" android:text="EOT : 150" android:layout_toRightOf="@+id/textIT" android:layout_alignBaseline="@+id/textAFR"/>
            <TextView android:textSize="13dp" android:textColor="#000000" android:id="@+id/textBAT" android:layout_width="80dp" android:layout_height="25dp" android:text="BAT : 14.5" android:layout_toRightOf="@+id/textEOT" android:layout_alignBaseline="@+id/textAFR"/>
        </RelativeLayout>
    </HorizontalScrollView>
</RelativeLayout>
